// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Student{
  id  String @id @default(uuid())
  name String
  age String
  major String
   classroom Classroom @relation(fields: [id], references: [id])

}

model Classroom{
  id String @id 
  name String
  teacher Teacher[]
  student Student[]
}

model Teacher{
  id String @id 
  name String
  classroom Classroom @relation(fields: [id], references: [id])
  
}

model User{
  id String @id @default(uuid())
  name String
  email String @unique
  password String
}